{
  "name": "@goldn/data-product",
  "version": "0.0.0",
  "main": "src/index.ts",
  "sideEffects": [
    "./src/graphqlClient.ts"
  ],
  "dependencies": {
    "axios": "^0.24.0",
    "graphql": "15.5.0",
    "graphql-request": "3.4.0",
    "react": "17.0.2",
    "swr": "0.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@types/graphql": "14.5.0",
    "apollo": "2.32.10",
    "babel-loader": "^8.2.3"
  },
  "scripts": {
    "generate:delete": "find ./ -name '_generated' | xargs rm -Rf",
    "generate": "yarn generate:delete && apollo codegen:generate --localSchemaFile=./types/graphql-schema.json --target=typescript --includes=./**/*.ts --tagName=gql --addTypename --globalTypesFile=./types/graphql-global-types.ts _generated",
    "schema": "apollo schema:download --endpoint=http://localhost:8000/graphql/ ./types/graphql-schema.json"
  }
}
